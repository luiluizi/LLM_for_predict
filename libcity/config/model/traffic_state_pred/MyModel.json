{
    "enc_drop": 0.3,
    "enc_dilation_exponential": 1,
    "enc_conv_channels": 32,
    "enc_residual_channels": 32,
    "enc_skip_channels": 64,
    "enc_end_channels": 128,
    "enc_layers": 3,

    "llm_lin_hidden_size": 128,
    "llm_time_steps": 12,
    "st_hidden_size": 64,


    "scaler": "standard",
    "load_external": true,
    "normal_external": false,
    "ext_scaler": "none",
    "add_time_in_day": false,
    "add_day_in_week": false,
    "batch_size": 4,

    "seed": 0,
    "max_epoch": 3,
    "learner": "adamw",
    "learning_rate": 2e-3,
    "weight_decay": 0.05,
    "lr_decay": true,
    "lr_scheduler": "cosinelr",
    "lr_eta_min": 1e-4,
    "lr_decay_ratio": 0.1,
    "lr_warmup_epoch": 5,
    "lr_warmup_init": 2e-3,
    "clip_grad_norm": true,
    "max_grad_norm": 5,
    "use_early_stop": true,
    "patience": 50,
    "step_size": 1562,
    "task_level": 0,
    "use_curriculum_learning": true,
    "random_flip": true,
    "huber_delta": 1,
    "quan_delta": 0.25,
    "bidir": false,
    "far_mask_delta": 5,
    "dtw_delta": 5,
    "set_loss": "masked_mae",
    "enc_pretrain": true
}